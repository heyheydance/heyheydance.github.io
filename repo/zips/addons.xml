<?xml version='1.0' encoding='utf-8'?>
<addons><addon id="plugin.program.heyheydancewizard" name="Heyheydance Wizard" version="1.0.0" provider-name="heyheydance">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
        <import addon="script.module.requests" />
        <import addon="script.module.six" />
        <import addon="script.kodi.android.update" optional="true" version="1.1.7" />
        <import addon="script.speedtester" optional="true" version="1.1.2" />
    </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
        <summary lang="en">[COLOR green]Heyheydance[/COLOR]Wizard</summary>
        <description lang="en">[COLOR green]Heyheydance[/COLOR]Wizard offers pre-set builds mainly for friends and family</description>
        <platform>all</platform>
  </extension>
</addon><addon id="repository.heyheydancerepo" name="[COLOR green]Heyheydance's Repository[/COLOR]" version="1.0.0" provider-name="heyheydance">
    <extension point="xbmc.addon.repository" name="Heyheydance Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/heyheydance/heyheydance.github.io/master/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/heyheydance/heyheydance.github.io/master/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/heyheydance/heyheydance.github.io/master/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Heyheydance's Repo</summary>
        <description>Repo for HeyheydanceWizard</description>
        <disclaimer />
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon></addons>